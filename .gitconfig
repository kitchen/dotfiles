[user]
	name = Jeremy Kitchen
	email = kitchen@kitchen.io
	signingkey = 0xB6F0E4F4
[color]
	ui = auto
[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	l2 = log --graph --all --abbrev-commit --pretty=oneline --decorate
	commit = commit -S
[core]
[push]
	default = current
[include]
	path = ~/.gitconfig.local
[init]
	templatedir = ~/.git-init-template

[filter "ssh_space_commander_config"]
	clean = ~/bin/filter-ssh-config.awk
	blur = cat
[url "git@github.com:"]
	insteadOf = https://github.com/
